<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <description>GetCaptcha</description>
    <display-name>GetCaptcha</display-name>
    <servlet-name>GetCaptcha</servlet-name>
    <servlet-class>servlet.GetCaptcha</servlet-class>
  </servlet>
  <servlet>
    <description>CheckLogin</description>
    <display-name>CheckLogin</display-name>
    <servlet-name>CheckLogin</servlet-name>
    <servlet-class>servlet.CheckLogin</servlet-class>
  </servlet>
  <servlet>
    <description>CheckRegister</description>
    <display-name>CheckRegister</display-name>
    <servlet-name>CheckRegister</servlet-name>
    <servlet-class>servlet.CheckRegister</servlet-class>
  </servlet>
  <servlet>
    <description>AddTopic</description>
    <display-name>AddTopic</display-name>
    <servlet-name>AddTopic</servlet-name>
    <servlet-class>servlet.AddTopic</servlet-class>
  </servlet>
  <servlet>
    <description>LoginOut</description>
    <display-name>LoginOut</display-name>
    <servlet-name>LoginOut</servlet-name>
    <servlet-class>servlet.LoginOut</servlet-class>
  </servlet>
  <servlet>
    <description>AddReply</description>
    <display-name>AddReply</display-name>
    <servlet-name>AddReply</servlet-name>
    <servlet-class>servlet.AddReply</servlet-class>
  </servlet>
  <servlet>
    <description>TopTopic</description>
    <display-name>TopTopic</display-name>
    <servlet-name>TopTopic</servlet-name>
    <servlet-class>servlet.TopTopic</servlet-class>
  </servlet>
  <servlet>
    <description>TopTopic</description>
    <display-name>TopTopic</display-name>
    <servlet-name>c</servlet-name>
    <servlet-class>servlet.c</servlet-class>
  </servlet>
  <servlet>
    <description>DeleteTopic</description>
    <display-name>DeleteTopic</display-name>
    <servlet-name>DeleteTopic</servlet-name>
    <servlet-class>servlet.DeleteTopic</servlet-class>
  </servlet>
  <servlet>
    <description>DeleteReply</description>
    <display-name>DeleteReply</display-name>
    <servlet-name>DeleteReply</servlet-name>
    <servlet-class>servlet.DeleteReply</servlet-class>
  </servlet>
  <servlet>
    <description>CancelTopTopic</description>
    <display-name>CancelTopTopic</display-name>
    <servlet-name>CancelTopTopic</servlet-name>
    <servlet-class>servlet.CancelTopTopic</servlet-class>
  </servlet>















  <servlet-mapping>
    <servlet-name>GetCaptcha</servlet-name>
    <url-pattern>/GetCaptcha</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckLogin</servlet-name>
    <url-pattern>/CheckLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckRegister</servlet-name>
    <url-pattern>/CheckRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddTopic</servlet-name>
    <url-pattern>/AddTopic</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginOut</servlet-name>
    <url-pattern>/LoginOut</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddReply</servlet-name>
    <url-pattern>/AddReply</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TopTopic</servlet-name>
    <url-pattern>/TopTopic</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteTopic</servlet-name>
    <url-pattern>/DeleteTopic</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteReply</servlet-name>
    <url-pattern>/DeleteReply</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CancelTopTopic</servlet-name>
    <url-pattern>/CancelTopTopic</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>welcome.jsp</welcome-file>
  </welcome-file-list>
  
  <session-config>
  	<session-timeout>15</session-timeout>
  </session-config>
</web-app>
